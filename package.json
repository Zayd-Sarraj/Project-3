{
  "name": "gsn",
  "version": "1.0.0",
  "description": "gaming social netwok",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\" \"npm run seed\" ",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedUsersDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zaydorade/Project-3.git"
  },
  "keywords": [
    "gsn"
  ],
  "author": "Zayd Sarraj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zaydorade/Project-3/issues"
  },
  "homepage": "https://github.com/Zaydorade/Project-3#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.7",
    "multer": "^1.4.2",
    "nodemon": "^2.0.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7"
  }
}
